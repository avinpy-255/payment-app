{
  "version": 2,
  "name": "payment-backend",
  "builds": [
     { "src": "*.js", "use": "@vercel/node" }
  ],
  "routes": [
     { "src": "/(.*)",
    "dest": "index.js",
    "origin": "https://payment-app-frontend-git-main-avinpy-255s-projects.vercel.app",
     "methods": ["GET", "PUT", "DELETE"],
     "headers": { "Access-Control-Allow-Origin": "*" }
     }
  ]
}